#
# RTSP Kconfig
#

menu "RTSP Server and Client"

config RTSP
    bool "implementation rtsp"
    default n
    help
      Implementation rtsp

config RTSP_SERVER_CLIENT
    bool "rtsp server and client"
    default n
    depends on RTSP
    help
      Enable rtsp server and client.

config SIMPLE_RTSP_SERVER
    bool "simple rtsp server with c language"
    default n
    depends on RTSP
    help
      Enable simple rtsp server.

config LIVE555_RTSP_SERVER
    bool "live555 rtsp server"
    default n
    depends on RTSP
    select LIVE555
    help
      Enable live555 rtsp server.

endmenu
